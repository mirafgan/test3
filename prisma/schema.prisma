generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          Int    @id @default(autoincrement())
  name        String
  description String
}

model Category {
  id        Int      @id @default(autoincrement())
  vegetable String   @unique
  cars      String[]
}
